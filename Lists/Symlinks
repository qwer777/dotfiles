$DOTFILES_HOME_FOLDER/.env=$HOME/.env
$DOTFILES_HOME_FOLDER/.nano=$HOME/.nano
$DOTFILES_HOME_FOLDER/.nanorc=$HOME/.nanorc
$DOTFILES_HOME_FOLDER/.screenrc=$HOME/.screenrc
$DOTFILES_HOME_FOLDER/zsh/.zshenv=$HOME/.zshenv
